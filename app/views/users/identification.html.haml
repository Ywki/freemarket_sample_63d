.indenti-header
  = render "items/header" 
  .indenti-wrapper
    .indenti-crumbs
      %ul
        %li
          = link_to "メルカリ", "https://www.yahoo.co.jp/"
          %i.icon-arrow
            = fa_icon "chevron-right"
        %li
          = link_to "マイページ", "https://www.yahoo.co.jp/"
          %i.icon-arrow
            = fa_icon "chevron-right"
        %li
          %span.indenti-list{itemprop: "title"} 本人情報の登録
    .clearfix
      .sell-icon
        = link_to "" do
          .red-circle
            %p.text 出品
            = fa_icon 'camera', class: "camera"
      .indenti-container
        .indenti-content
          .mypage-indenti
            %h2.indenti-chapter 本人情報の登録
            %form.indenti-forms
              %div
                %p
                  お客さまの本人情報をご登録ください。
                  %br>/
                  登録されたお名前・生年月日を変更する場合、本人確認書類の提出が必要になります。
                %p.text-right
                  = link_to "本人確認書類のアップロードについて", "https://www.yahoo.co.jp/"
                  %i.icon-arrow-right
                    = fa_icon "chevron-right"
              %dev.indenti-form
                %label お名前
                %p Dチーム
              %div.indenti-form
                %label お名前カナ
                %p ディーチーム
              %div.indenti-form
                %label 生年月日
                %p 7/4
              %div.indenti-form
                %label 郵便番号
                %span.form-arbitray 任意
                %br>/
                %input.input-default{placeholder: "例) 1234567", type: "text", value: ""}/
              %div.indenti-form
                %label 都道府県
                %span.form-arbitray 任意
                %div.select-wrap
                  %i.icon-arrow-bottom 
                    = fa_icon "chevron-down"
                  %select.select-default{name: "prefecture"}
                    %option --
                    %option 北海道
                    %option 青森県
                    %option 岩手県
                    %option 宮城県
                    %option 秋田県
                    %option 山形県
                    %option 福島県
                    %option 東京都
                    %option 神奈川県
                    %option 埼玉県
                    %option 千葉県
                    %option 茨城県
                    %option 栃木県
                    %option 群馬県
                    %option 山梨県
                    %option 新潟県
                    %option 長野県
                    %option 富山県
                    %option 石川県
                    %option 福井県
                    %option 愛知県
                    %option 岐阜県
                    %option 静岡県
                    %option 三重県
                    %option 大阪府
                    %option 兵庫県
                    %option 京都府
                    %option 滋賀県
                    %option 奈良県
                    %option 和歌山県
                    %option 鳥取県
                    %option 島根県
                    %option 岡山県
                    %option 広島県
                    %option 山口県
                    %option 徳島県
                    %option 香川県
                    %option 愛媛県
                    %option 高知県
                    %option 福岡県
                    %option 佐賀県
                    %option 長崎県
                    %option 熊本県
                    %option 大分県
                    %option 宮崎県
                    %option 鹿児島県
                    %option 沖縄県

              %div.indenti-form
                %label 市区町村
                %span.form-arbitray 任意
                %br>/
                %input.input-default{type: "text", name: "city", placeholder: "例) 横浜市緑区"}
              %div.indenti-form
                %label 番地
                %span.form-arbitray 任意
                %br>/
                %input.input-default{type: "text", name: "address1", placeholder: "例) 青山1-1-1"}
              %div.indenti-form
                %label 建物名
                %span.form-arbitray 任意
                %br/
                %input.input-default{type: "text", name: "address2", placeholder: "例) 柳ビル103"}
              %button.btn-indenti{type: "submit"}登録する
              .indenti-form-text
                = link_to "本人情報の登録について", "https://www.yahoo.co.jp/"
                %i.icon-arrow-right
                  = fa_icon "chevron-right"
        .indenti-side
          .mains
            .left
              .left__top
                %ul
                  = link_to user_path do
                    %p
                      マイページ
                      %i= fa_icon "chevron-right", class: "special"
                  %li
                    お知らせ
                    %i= fa_icon "chevron-right"
                  %li
                    やることリスト
                    %i= fa_icon "chevron-right"
                  %li
                    いいね！一覧
                    %i= fa_icon "chevron-right"
                  %li
                    出品する
                    %i= fa_icon "chevron-right"
                  %li
                    出品した商品・出品中
                    %i= fa_icon "chevron-right"
                  %li
                    出品した商品・取引中
                    %i= fa_icon "chevron-right"
                  %li
                    出品した商品・売却済み
                    %i= fa_icon "chevron-right"
                  %li
                    購入した商品・取引中
                    %i= fa_icon "chevron-right"
                  %li
                    購入した商品・過去の取引
                    %i= fa_icon "chevron-right"
                  %li
                    ニュース一覧
                    %i= fa_icon "chevron-right"
                  %li
                    評価一覧
                    %i= fa_icon "chevron-right"
                  %li
                    ガイド
                    %i= fa_icon "chevron-right"
                  %li
                    お問い合わせ
                    %i= fa_icon "chevron-right"
              .left__center
                メルペイ
                .left__center__box
                  %li
                    売上・振込申請
                    %i= fa_icon "chevron-right"
                  %li
                    ポイント
                    %i= fa_icon "chevron-right"
              .left__bottom
                設定
                .left__bottom__box
                  %li
                    プロフィール
                    %i= fa_icon "chevron-right"
                  %li
                    発送元・お届け先住所変更
                    %i= fa_icon "chevron-right"
                  %li
                    支払い方法
                    %i= fa_icon "chevron-right"
                  %li
                    メール/パスワード
                    %i= fa_icon "chevron-right"
                  %li
                    本人情報
                    %i= fa_icon "chevron-right"
                  %li
                    電話番号の確認
                    %i= fa_icon "chevron-right"
                  %li
                    ログアウト
                    %i= fa_icon "chevron-right" 
  .indenti-banner
    = render "items/footer"
